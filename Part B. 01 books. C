#include <stdio.h>
int main() {
    int books[100], n, key, low, high, mid;
    printf("Enter number of books: ");
    scanf("%d", &n);
    printf("Enter %d Book IDs in ascending order:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &books[i]);
    }
    printf("Enter Book ID to search: ");
    scanf("%d", &key);
    low = 0;
    high = n - 1;
    int found = 0;
    while(low <= high) {
        mid = (low + high) / 2;
        if(key == books[mid]) {
            found = 1;
            break;
        }
        else if(key < books[mid]) {
            high = mid - 1;
        }
        else {
            low = mid + 1;
        }
    }
    if(found)
        printf("Book with ID %d is AVAILABLE.\n", key);
    else
        printf("Book with ID %d is NOT AVAILABLE.\n", key);

    return 0;
}
