#include<stdio.h>
int main() {
    int a,b,c;
    printf("Enter three number");
    scanf("%d%d%d\n",&a,&b,&c);
    if (a<b && a<c)
        printf("%d is the smallest\n",a);
        if else (b<c)
        printf("%d is the smallest \n",b);
        else
        printf("%d is the smallest \n",c);
    }
