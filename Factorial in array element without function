#include <stdio.h>
int main() {
    int n, i, j;
    long long fact;
    printf("Enter size of array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter %d elements: ", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("\nFactorial of each element:\n");
    for(i = 0; i < n; i++) {
        fact = 1;
        for(j = 1; j <= arr[i]; j++) {
            fact *= j;
        }
        printf("Factorial of %d = %lld\n", arr[i], fact);
    }
    return 0;
}
