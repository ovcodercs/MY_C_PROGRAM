#include <stdio.h>
int main() {
    int u1, u2, p1, p2, total;
    printf("Enter units of product1 and product2: ");
    scanf("%d %d", &u1, &u2);
    printf("Enter price of product1 and product2: ");
    scanf("%d %d", &p1, &p2);
    total = (u1 * p1) + (u2 * p2);
    printf("Total revenue = %d", total);
    return 0;
}
