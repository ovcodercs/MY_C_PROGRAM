#include <stdio.h>
int main() {
    int pan = 73535;
    int aadhar = 11111;
    int apaar = 11122;
    int dl = 33344;       
    int passport = 55566;
    int input;
    int choice;
    printf("KYC Verification System\n");
    printf("------------------------\n");
    printf("Select ID type to verify:\n");
    printf("1. PAN Number\n");
    printf("2. AADHAR Number\n");
    printf("3. APAAR Id\n");
    printf("4. Driving License\n");
    printf("5. Passport\n");
    printf("Enter your choice: ");
    scanf("%d", &choice);
    printf("Enter the ID Number: ");
    scanf("%d", &input);
    switch(choice) {
        case 1:
            if (input == pan)
                printf("PAN Verified Successfully!\n");
            else
                printf("PAN Verification Failed!\n");
            break;
        case 2:
            if (input == aadhar)
                printf("AADHAR Verified Successfully!\n");
            else
                printf("AADHAR Verification Failed!\n");
            break;
        case 3:
            if (input == apaar)
                printf("APAAR ID Verified Successfully!\n");
            else
                printf("APAAR ID Verification Failed!\n");
            break;
        case 4:
            if (input == dl)
                printf("Driving License Verified Successfully!\n");
            else
                printf("Driving License Verification Failed!\n");
            break;
        case 5:
            if (input == passport)
                printf("Passport Verified Successfully!\n");
            else
                printf("Passport Verification Failed!\n");
            break;
        default:
            printf("Invalid Choice!\n");
    }
    return 0;
}
